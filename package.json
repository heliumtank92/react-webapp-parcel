{
  "name": "react-webapp-parcel",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "analyze": "pnpm run clean && parcel build public/index.ejs --dist-dir build --reporter @parcel/reporter-bundle-analyzer",
    "start": "pnpm run clean && parcel public/index.ejs --dist-dir build --open",
    "build": "pnpm run clean && parcel build public/index.ejs --dist-dir build",
    "clean": "rm -rf build .parcel-cache"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@am92/react-design-system": "^0.0.14",
    "@am92/web-http": "^1.1.1",
    "@parcel/service-worker": "^2.8.3",
    "@reduxjs/toolkit": "^1.9.3",
    "buffer": "^5.5.0",
    "localforage": "^1.10.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.8.3",
    "@parcel/core": "^2.8.3",
    "@parcel/packager-raw-url": "^2.8.3",
    "@parcel/packager-xml": "^2.8.3",
    "@parcel/reporter-bundle-analyzer": "^2.8.3",
    "@parcel/transformer-raw": "^2.8.3",
    "custom-env": "^2.0.1",
    "parcel": "^2.8.3",
    "parcel-config-pwa-manifest": "^0.1.2",
    "parcel-namer-rewrite": "2.0.0-rc.3",
    "parcel-reporter-copy-file-v2": "^1.0.1",
    "parcel-transformer-ejs": "^1.0.1",
    "process": "^0.11.10",
    "sharp": "^0.31.3"
  },
  "parcel-namer-rewrite": {
    "rules": {
      "(.*).css$": "styles/$1{.hash}.css",
      "service-worker.js": "service-worker.js",
      "(.*).js$": "scripts/$1{.hash}.js",
      "(.*).png$": "assets/images/$1{.hash}.png",
      "(.*).webp$": "assets/images/$1{.hash}.webp",
      "(.*).jpg$": "assets/images/$1{.hash}.jpg",
      "(.*).jpeg$": "assets/images/$1{.hash}.jpeg",
      "(.*).svg$": "assets/images/$1{.hash}.svg"
    },
    "profiles": {
      "development": {
        "hashing": "always"
      },
      "production": {
        "hashing": "always"
      }
    }
  },
  "copyStaticFiles": [
    "public/static/*",
    "public/robots.txt"
  ]
}
